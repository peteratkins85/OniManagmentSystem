{# app/Resources/views/base.html.twig #}
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>{% block title %}{% endblock %}</title>
        <link href="{{ asset('lib/dependencies/bootstrap/dist/css/bootstrap-theme.css') }}" rel="stylesheet" type="text/css"/>
        <link href="{{ asset('lib/dependencies/bootstrap/dist/css/bootstrap.css') }}" rel="stylesheet" type="text/css"/>
        {% block stylesheets %}{% endblock %}
        <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}
        {% endblock %}
        {% block javascripts %}
            {# Bootstrap include #}
            <script src="lib/dependencies/jquery/jquery.min.js"></script>
            <script  src="{{ asset('lib/dependencies/bootstrap/dist/js/bootstrap.min.js') }}" type="text/javascript"></script>
        {% endblock %}
        {% block custom_javascript %}
        {% endblock %}
    </body>
</html>