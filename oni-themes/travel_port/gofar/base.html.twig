{# app/Resources/views/base.html.twig #}
<!DOCTYPE html>
<html>
<head>
    <title>{% block title %}{% endblock %}</title>
    <meta content='text/html;charset=utf-8' http-equiv='content-type'>
    <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
    {{ include('@travel_port/gofar/head.html.twig') }}
    {% block stylesheets %}{% endblock %}
</head>
<!--[if IE 7]> <body class="ie7 lt-ie8 lt-ie9 lt-ie10"> <![endif]-->
<!--[if IE 8]> <body class="ie8 lt-ie9 lt-ie10"> <![endif]-->
<!--[if IE 9]> <body class="ie9 lt-ie10"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <body> <!--<![endif]-->
    <div id="page-wrap">
        <!-- PRELOADER -->
        <div class="preloader"></div>
        <!-- END / PRELOADER -->
        {{ include('@travel_port/gofar/header.html.twig') }}
        {% block body %}

        {% endblock %}
        {% block footer %}
            {{ include('@travel_port/gofar/footer.html.twig') }}
        {% endblock %}
    </div>
    {{ include('@travel_port/gofar/javascript-include.html.twig') }}
    {% block javascripts %}
        <!-- JS Implementing Plugins -->
        <script type="text/javascript" src="{{ asset('/oni-themes/travel_port/gofar/js/lib/jquery-1.11.2.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('/oni-themes/travel_port/gofar/js/lib/masonry.pkgd.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('/oni-themes/travel_port/gofar/js/lib/jquery.parallax-1.1.3.js') }}"></script>
        <script type="text/javascript" src="{{ asset('/oni-themes/travel_port/gofar/js/lib/jquery.owl.carousel.js') }}"></script>
        <script type="text/javascript" src="{{ asset('/oni-themes/travel_port/gofar/js/lib/theia-sticky-sidebar.js') }}"></script>
        <script type='text/javascript' src="{{ asset('/oni-themes/travel_port/gofar/js/lib/jquery-ui.js') }}"></script>
        <script type="text/javascript" src="{{ asset('/oni-themes/travel_port/gofar/js/scripts.js') }}"></script>
    {% endblock %}
</body>
</html>